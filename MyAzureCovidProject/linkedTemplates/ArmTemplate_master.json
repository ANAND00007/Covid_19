{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MyAzureCovidProject"
		},
		"Is_link_for_datalakestorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Is_link_for_datalakestorage'"
		},
		"Is_link_from_blobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Is_link_from_blobstorage'"
		},
		"Is_link_for_datalakestorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidprojectdatalake.dfs.core.windows.net/"
		},
		"trigger_ingest_pop_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/a4b5f70f-5bb5-4716-bf6d-f64dfdc4d044/resourceGroups/Covid_project/providers/Microsoft.Storage/storageAccounts/covidprojectdatastorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "MyAzureCovidProject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Is_link_for_datalakestorage_accountKey": {
						"value": "[parameters('Is_link_for_datalakestorage_accountKey')]"
					},
					"Is_link_from_blobstorage_connectionString": {
						"value": "[parameters('Is_link_from_blobstorage_connectionString')]"
					},
					"Is_link_for_datalakestorage_properties_typeProperties_url": {
						"value": "[parameters('Is_link_for_datalakestorage_properties_typeProperties_url')]"
					},
					"trigger_ingest_pop_data_properties_typeProperties_scope": {
						"value": "[parameters('trigger_ingest_pop_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}